{
  "name": "SnowShoe-Swift",
  "version": "2.0.0",
  "summary": "iOS wrapper for SnowShoe in Swift 3",
  "description": "SnowShoe is an authentication tool for smartphones. This library provides a simple Swift wrapper.",
  "homepage": "https://github.com/snowshoestamp/snowshoe-swift-3",
  "license": "MIT",
  "authors": {
    "SnowShoe": "support@snowshoestamp.com"
  },
  "source": {
    "git": "https://github.com/snowshoestamp/snowshoe-swift-3.git",
    "tag": "2.0.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "SnowShoe-Swift": [
      "Pod/Assets/*.png"
    ]
  },
  "dependencies": {
    "OAuthSwift": [
      "1.2.0"
    ],
    "ObjectMapper": [
      "3.1.0"
    ]
  }
}
